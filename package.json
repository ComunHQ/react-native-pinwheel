{
  "name": "@pinwheel/react-native-pinwheel",
  "version": "3.0.0",
  "type": "module",
  "description": "Pinwheel React Native SDK",
  "main": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "scripts": {
    "build": "tsc",
    "bump-pkg-version": "./scripts/bump-pkg-version.sh",
    "dev": "./scripts/dev.sh",
    "prepare": "bob build"
  },
  "author": "Pinwheel",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.13.1 || ^17 || ^18",
    "react-native": "*",
    "react-native-webview": "*"
  },
  "devDependencies": {
    "@types/react-native": "^0.72.2",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^4.0.3"
  },
  "react-native": "dist/module/index.js",
  "source": "dist/module/index.js",
  "module": "dist/module/index.js",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
